#: forms.py:84
msgid "Enter a post code in the format XXXXXX."
msgstr "Zadajte poštové smerovacie číslo v tvare XXXXXX."

#: forms.py:105
msgid "ID Card Number consists of 15 or 18 digits."
msgstr "Číslo preukazu sa skladá z 15 alebo 18 číslic."

#: forms.py:106
msgid "Invalid ID Card Number: Wrong checksum"
msgstr "Neplatné číslo preukazu: Zlý kontrolný súčet"

#: forms.py:107
msgid "Invalid ID Card Number: Wrong birthdate"
msgstr "Neplatné číslo preukazu: Zlý dátum narodenia"

#: forms.py:108
msgid "Invalid ID Card Number: Wrong location code"
msgstr "Neplatné číslo preukazu: Zlý kód územia"

#: forms.py:193
msgid "Enter a valid phone number."
msgstr "Zadajte platné telefónne číslo."

#: forms.py:210
msgid "Enter a valid cell number."
msgstr "Zadajte platné číslo mobilného telefónu."
