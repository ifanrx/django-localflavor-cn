msgid "Enter a post code in the format XXXXXX."
msgstr "Vnesite poštno številko v obliki XXXXXX."

#: forms.py:105
msgid "ID Card Number consists of 15 or 18 digits."
msgstr "Številka ID kartice je sestavljena iz 15 ali 18 števk."

#: forms.py:106
msgid "Invalid ID Card Number: Wrong checksum"
msgstr "Neveljavna številka ID kartice: napačna preverjevalna števka."

#: forms.py:107
msgid "Invalid ID Card Number: Wrong birthdate"
msgstr "Neveljavna številka ID kartice: neveljaven datum rojstva."

#: forms.py:108
msgid "Invalid ID Card Number: Wrong location code"
msgstr "Neveljavna številka ID kartice: neveljavna lokacijska koda."

#: forms.py:193
msgid "Enter a valid phone number."
msgstr "Vnesite veljavno telefonsko številko."

#: forms.py:210
msgid "Enter a valid cell number."
msgstr "Vnesite veljavno mobilno številko."
